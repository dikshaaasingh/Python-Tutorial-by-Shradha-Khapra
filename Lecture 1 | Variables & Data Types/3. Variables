name= "Diksha" #String
age= 23 #Integer
marks= 97.97 #Float

print(name)
print(age)
print(marks)

PRINT("--------------------------------------------------------------------------")
#Another way to print output
print("My name is:", name)
print("My age is", age)
print("My marks are", marks)

